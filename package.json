{
	"name": "wp-ever-accounting",
	"version": "1.0.1",
	"description": "The Best WordPress Accounting Plugin ever made!",
	"keywords": [],
	"homepage": "https://www.pluginever.com",
	"author": {
		"name": "pluginever",
		"email": "support@pluginever.com",
		"url": "https://www.pluginever.com"
	},
	"main": "index.js",
	"scripts": {
		"start": "npm run clean && cross-env npm run build:packages && concurrently \"cross-env  webpack --watch\" \"node ./bin/packages/watch.js\"",
		"clean": "rimraf ./assets/dist ./packages/*/build ./packages/*/build-module ./packages/*/build-style",
		"prebuild": "npm run -s install-if-deps-outdated && php ./bin/update-version.php",
		"build": "npm run clean && npm run build:packages && npm run build:core",
		"build:core": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
		"postbuild": "npm run -s i18n:php && npm run -s i18n:pot",
		"build:packages": "node ./bin/packages/build.js",
		"build:release": "./bin/build-plugin-zip.sh",
		"publish-packages:check": "npm run build:packages && lerna updated",
		"publish-packages:dev": "npm run build:packages && lerna publish from-package --dist-tag next",
		"publish-packages:prod": "npm run build:packages && lerna publish from-package",
		"install-if-deps-outdated": "node bin/install-if-deps-outdated.js",
		"install-if-no-packages": "node bin/install-if-no-packages.js",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses --prod --gpl2 --ignore=abab",
		"format:js": "wp-scripts format-js ./client",
		"i18n": "npm run -s i18n:js && npm run -s i18n:check && npm run -s i18n:pot && npm run -s i18n:build",
		"i18n:build": "php bin/combine-pot-files.php languages/wp-ever-accounting.po languages/wp-ever-accounting.pot",
		"i18n:check": "grunt checktextdomain",
		"i18n:js": "npm run clean && cross-env NODE_ENV=production babel client packages -o /dev/null",
		"i18n:json": "./bin/make-i18n-json.sh",
		"i18n:pot": "grunt makepot",
		"lint": "npm run lint:js && npm run lint:css",
		"lint:css": "stylelint 'client/**/*.scss'",
		"lint:css-fix": "stylelint '**/*.scss' --fix",
		"lint:js": "eslint packages --ext=js,jsx && eslint client --ext=js,jsx",
		"lint:js-fix": "wp-scripts lint-js packages/**/src --ext=js,jsx --fix && wp-scripts lint-js client --ext=js,jsx --fix",
		"lint:php": "./vendor/bin/phpcs --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
		"lint:php-fix": "./vendor/bin/phpcbf --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
		"reformat-files": "prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,json,ts,tsx}\"",
		"create-extension": "node ./bin/starter-pack/starter-pack.js",
		"format": "wp-scripts format-js ./client"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.28",
		"@fortawesome/free-solid-svg-icons": "^5.13.0",
		"@fortawesome/react-fontawesome": "^0.1.9",
		"accounting-js": "^1.1.1",
		"bootstrap-daterangepicker": "^3.0.5",
		"chart.js": "^2.9.3",
		"cuid": "^2.1.8",
		"decimal.js-light": "^2.5.0",
		"final-form": "^4.18.7",
		"find-and": "^1.3.0",
		"gridicons": "^3.3.1",
		"papaparse": "^5.2.0",
		"puppeteer": "^2.1.1",
		"react-chartjs-2": "^2.9.0",
		"react-final-form": "^6.3.5",
		"react-moment": "^0.9.7",
		"react-notifications": "^1.5.0-beta.0",
		"react-number-format": "^4.4.1",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-select": "^3.0.8",
		"warning": "^4.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@eaccounting/components": "file:./packages/components",
		"@eaccounting/data": "file:./packages/data",
		"@eaccounting/hoc": "file:./packages/hoc",
		"@eaccounting/store": "file:./packages/store",
		"@wordpress/babel-plugin-makepot": "^3.4.0",
		"@wordpress/babel-preset-default": "^4.10.0",
		"@wordpress/custom-templated-path-webpack-plugin": "^1.6.0",
		"@wordpress/scripts": "^7.2.0",
		"autoprefixer": "^9.7.4",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"chalk": "^3.0.0",
		"concurrently": "^5.1.0",
		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^7.0.2",
		"css-loader": "^3.4.2",
		"deasync": "^0.1.19",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"file-loader": "^5.1.0",
		"fs": "0.0.1-security",
		"grunt": "^1.0.4",
		"grunt-checktextdomain": "^1.0.1",
		"grunt-wp-i18n": "^1.0.3",
		"lerna": "^3.20.2",
		"mini-css-extract-plugin": "^0.9.0",
		"mkdirp": "^1.0.3",
		"node-sass": "^4.13.1",
		"node-watch": "^0.6.3",
		"postcss-color-function": "^4.1.0",
		"postcss-focus": "^4.0.0",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"postcss-reporter": "^6.0.1",
		"precss": "^4.0.0",
		"prettier": "npm:wp-prettier@^1.19.1",
		"progress-bar-webpack-plugin": "^2.1.0",
		"prop-types": "^15.7.2",
		"rimraf": "^3.0.2",
		"sass-loader": "^8.0.2",
		"stylelint": "^13.2.1",
		"stylelint-config-wordpress": "^16.0.0",
		"terser-webpack-plugin": "^2.3.5",
		"webpack-fix-style-only-entries": "^0.4.0",
		"webpack-remove-debug": "^0.1.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=8.9.3",
		"npm": ">=5.5.1"
	},
	"esversion": 6,
	"title": "WP Ever Accounting"
}
