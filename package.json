  },
  "files": [
    "dist/**/*.{js,scss}",
    "languages/**/*.json",
    "license.txt"
  ],
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.14.2"
  },
  "dependencies": {
    "moment": "^2.29.1",
