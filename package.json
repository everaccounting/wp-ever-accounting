{
	"name": "wp-ever-accounting",
	"title": "WP Ever Accounting",
	"description": "The Best WordPress Accounting Plugin ever made!",
	"version": "1.0.1",
	"homepage": "https://www.pluginever.com",
	"keywords": [],
	"main": "index.js",
	"author": {
		"name": "pluginever",
		"email": "support@pluginever.com",
		"url": "https://www.pluginever.com"
	},
	"engines": {
		"node": ">=8.9.3",
		"npm": ">=5.5.1"
	},
	"esversion": 6,
	"scripts": {
		"prebuild": "npm run -s install-if-deps-outdated && php ./bin/update-version.php",
		"build": "npm run build:packages && npm run build:core",
		"build:core": "cross-env NODE_ENV=production webpack --display-error-details",
		"postbuild": "npm run -s i18n:php && npm run -s i18n:pot",
		"build:packages": "node ./bin/packages/build.js",
		"clean": "rimraf ./dist ./packages/*/build ./packages/*/build-module ./packages/*/build-style",
		"predev": "npm run -s install-if-deps-outdated && php ./bin/update-version.php",
	  	"package-plugin": "./bin/build-plugin-zip.sh",
		"dev": "cross-env npm run && cross-env npm run build:packages && cross-env webpack",
		"i18n": "npm run -s i18n:js && npm run -s i18n:php && npm run -s i18n:check && npm run -s i18n:pot",
		"i18n:check": "grunt checktextdomain",
		"i18n:js": "cross-env NODE_ENV=production babel client -o /dev/null",
		"i18n:php": "pot-to-php ./i18n/languages/wp-ever-accounting.pot ./i18n/languages/wp-ever-accounting.php wp-ever-accounting",
		"i18n:pot": "grunt makepot",
		"install-if-deps-outdated": "node bin/install-if-deps-outdated.js",
		"install-if-no-packages": "node bin/install-if-no-packages.js",
		"lint": "npm run lint:js && npm run lint:css",
		"lint:css": "stylelint '**/*.scss'",
		"lint:css-fix": "stylelint '**/*.scss' --fix",
		"lint:js": "npm run -s install-if-deps-outdated && eslint client --ext=js,jsx",
		"lint:php": "./vendor/bin/phpcs --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
		"lint:php-fix": "./vendor/bin/phpcbf --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
		"prepack": "npm install && npm run lint && npm run test && cross-env npm run build",
		"publish-packages:check": "npm run build:packages && lerna updated",
		"publish-packages:dev": "npm run build:packages && lerna publish from-package --npm-tag next",
		"publish-packages:prod": "npm run build:packages && lerna publish from-package",
		"prestart": "npm run -s install-if-deps-outdated && php ./bin/update-version.php",
		"start": "npm run clean && cross-env npm run build:packages && concurrently \"cross-env  webpack --watch\" \"node ./bin/packages/watch.js\"",
		"reformat-files": "./node_modules/.bin/prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,json}\"",
		"stat": "NODE_ENV=production webpack --json --mode production | webpack-bundle-size-analyzer",
		"watch": "cross-env npm run build:packages && concurrently \"cross-env  webpack --watch\" \"node ./bin/packages/watch.js\""
	},
	"dependencies": {
		"@automattic/wordpress-external-dependencies-plugin": "^1.0.0",
		"@babel/cli": "7.6.3",
		"@babel/core": "7.6.3",
		"@babel/plugin-proposal-class-properties": "7.4.4",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-syntax-dynamic-import": "7.2.0",
		"@babel/plugin-syntax-import-meta": "^7.2.0",
		"@babel/plugin-transform-react-constant-elements": "^7.5.0",
		"@babel/plugin-transform-react-inline-elements": "7.2.0",
		"@babel/plugin-transform-react-jsx": "^7.3.0",
		"@babel/preset-env": "7.4.5",
		"@babel/preset-react": "^7.6.3",
		"@eaccounting/components": "file:./packages/components",
		"@wordpress/babel-plugin-import-jsx-pragma": "^2.2.0",
		"@wordpress/babel-plugin-makepot": "^3.2.0",
		"@wordpress/babel-preset-default": "^4.6.2",
		"@wordpress/browserslist-config": "2.5.0",
		"@wordpress/components": "^8.1.0",
		"@wordpress/custom-templated-path-webpack-plugin": "^1.5.0",
		"@wordpress/hooks": "^2.6.0",
		"@wordpress/i18n": "^3.6.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"bootstrap-daterangepicker": "^3.0.5",
		"concurrently": "^5.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"cross-env": "5.2.0",
		"css-loader": "^3.2.0",
		"deasync": "^0.1.19",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"file-loader": "^4.2.0",
		"gridicons": "^3.3.1",
		"i18n-calypso-cli": "^1.0.0",
		"jed": "^1.1.1",
		"lerna": "^3.18.1",
		"locutus": "^2.0.11",
		"lodash-clean": "^2.2.1",
		"lru": "^3.1.0",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.12.0",
		"node-watch": "^0.6.3",
		"papaparse": "^5.1.1",
		"postcss-focus": "^4.0.0",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"postcss-reporter": "^6.0.1",
		"precss": "^4.0.0",
		"prettier": "^1.18.2",
		"progress-bar-webpack-plugin": "^2.1.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-bootstrap-datetimerangepicker": "^2.0.4",
		"react-currency-input": "^1.3.6",
		"react-dom": "^16.8.6",
		"react-highlight-words": "^0.16.0",
		"react-input-autosize": "^2.2.2",
		"react-input-color": "^2.0.29",
		"react-input-mask": "^2.0.4",
		"react-moment": "^0.9.2",
		"react-notifications": "^1.4.3",
		"react-router-dom": "^5.0.1",
		"react-select": "^3.0.8",
		"react-text-mask": "^5.4.3",
		"redux-thunk": "^2.3.0",
		"sass-loader": "^7.2.0",
		"style-loader": "^1.0.0",
		"text-mask-addons": "^3.8.0",
		"webpack": "4.34.0",
		"webpack-bundle-size-analyzer": "^3.0.0",
		"webpack-cli": "^3.3.7",
		"webpack-fix-style-only-entries": "^0.4.0"
	},
	"devDependencies": {
		"@wordpress/dependency-extraction-webpack-plugin": "^2.3.0",
		"autoprefixer": "~8.6.2",
		"grunt": "^1.0.3",
		"grunt-checktextdomain": "~1.0.1",
		"grunt-contrib-clean": "~1.1.0",
		"grunt-contrib-compress": "^1.4.3",
		"grunt-contrib-concat": "~1.0.1",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-cssmin": "~2.2.1",
		"grunt-contrib-jshint": "~1.1.0",
		"grunt-contrib-uglify": "~3.3.0",
		"grunt-contrib-watch": "^1.1.0",
		"grunt-phpcs": "^0.4.0",
		"grunt-postcss": "~0.9.0",
		"grunt-prompt": "^1.3.3",
		"grunt-sass": "^2.1.0",
		"grunt-stylelint": "~0.10.0",
		"grunt-wp-i18n": "^1.0.3",
		"stylelint": "^13.2.0"
	}
}
