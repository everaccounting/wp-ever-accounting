    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@byteever/eslint-plugin": "file:packages/eslint-plugin",
    "@eaccounting/dependency-extraction-webpack-plugin": "file:packages/dependency-extraction-webpack-plugin",
    "@wordpress/babel-plugin-makepot": "^4.1.2",
    "@wordpress/babel-preset-default": "^5.2.1",
    "@wordpress/browserslist-config": "^3.0.3",
    "@wordpress/custom-templated-path-webpack-plugin": "^2.0.3",
    "@wordpress/data": "^4.27.2",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.1.2",
    "@wordpress/dom-ready": "^2.13.2",
    "@wordpress/element": "^2.20.2",
    "@wordpress/i18n": "^3.19.2",
    "@wordpress/icons": "^2.10.2",
    "@wordpress/prettier-config": "^1.0.3",
    "@wordpress/stylelint-config": "^19.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "caniuse-db": "^1.0.30001214",
    "chalk": "^4.1.1",
    "classnames": "^2.3.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.0.2",
    "copy-webpack-plugin": "5.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cssnano": "^5.0.1",
    "cuid": "^2.1.8",
    "eslint-config-keystone": "^3.0.0",
    "eslint-config-keystone-react": "^1.0.0",
    "eslint-loader": "^4.0.2",
    "fast-glob": "^3.2.5",
    "git-clone": "^0.1.0",
    "glob": "^7.1.6",
    "grunt-checktextdomain": "^1.0.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-wp-i18n": "^1.0.3",
    "grunt-wp-readme-to-markdown": "^2.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "matchdep": "^2.0.0",
    "mini-css-extract-plugin": "^1.5.0",
    "mkdirp": "^1.0.4",
    "node-watch": "^0.7.1",
    "postcss": "^8.2.10",
    "postcss-color-function": "^4.1.0",
    "postcss-loader": "^4.2.0",
    "precss": "^4.0.0",
    "promptly": "^3.2.0",
