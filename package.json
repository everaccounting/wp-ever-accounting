{
  "name": "wp-ever-accounting",
  "title": "WP Ever Accounting",
  "version": "1.0.1",
  "description": "The Best WordPress Accounting Plugin ever made!",
  "keywords": [],
  "homepage": "https://www.pluginever.com",
  "author": {
	"name": "pluginever",
	"email": "support@pluginever.com",
	"url": "https://www.pluginever.com"
  },
  "main": "index.js",
  "scripts": {
	"start": "npm run clean && cross-env BABEL_ENV=default webpack --watch --info-verbosity none",
	"build": "npm run clean && cross-env BABEL_ENV=default NODE_ENV=production webpack",
	"explore": "source-map-explorer",
	"lint": "npm run lint:php && npm run lint:css && npm run lint:js",
	"lint:ci": "npm run lint:js && npm run lint:css",
	"lint:css": "stylelint 'assets/**/*.scss'",
	"lint:css-fix": "stylelint 'assets/**/*.scss' --fix",
	"lint:js": "wp-scripts lint-js",
	"lint:js-fix": "eslint assets/js --ext=js,jsx --fix",
	"reformat-files": "prettier --ignore-path .eslintignore --write \"**/*.{js,jsx,json,ts,tsx}\"",
	"clean": "rimraf ./assets/dist",
	"create-extension": "node ./bin/starter-pack/starter-pack.js",
	"format": "wp-scripts format-js ./assets/client"
  },
  "dependencies": {
	"@babel/core": "^7.11.6",
	"@babel/plugin-proposal-class-properties": "^7.10.4",
	"@babel/plugin-transform-runtime": "^7.11.5",
	"@wordpress/babel-plugin-makepot": "^3.8.0",
	"@wordpress/babel-preset-default": "^4.18.0",
	"@wordpress/custom-templated-path-webpack-plugin": "^1.7.0",
	"@wordpress/scripts": "^12.2.0",
	"chalk": "^4.1.0",
	"concurrently": "^5.3.0",
	"cross-env": "^7.0.2",
	"css-loader": "^4.2.2",
	"duplicate-package-checker-webpack-plugin": "^3.0.0",
	"file-loader": "^6.1.0",
	"mini-css-extract-plugin": "^0.11.0",
	"node-sass": "^4.14.1",
	"node-watch": "^0.6.4",
	"postcss-color-function": "^4.1.0",
	"postcss-focus": "^4.0.0",
	"postcss-loader": "^3.0.0",
	"postcss-preset-env": "^6.7.0",
	"postcss-reporter": "^6.0.1",
	"precss": "^4.0.0",
	"progress-bar-webpack-plugin": "^2.1.0",
	"prop-types": "^15.7.2",
	"rimraf": "^3.0.2",
	"sass-loader": "^10.0.2",
	"stylelint": "^13.7.0",
	"stylelint-config-wordpress": "^17.0.0",
	"webpack-fix-style-only-entries": "^0.5.1",
	"webpack-remove-debug": "^0.1.0",
	"@eaccounting/icons": "file:./client/icons"
  },
  "devDependencies": {
	"autoprefixer": "^9.8.6",
	"grunt": "1.3.0",
	"grunt-contrib-clean": "2.0.0",
	"grunt-contrib-concat": "1.0.1",
	"grunt-contrib-copy": "1.0.0",
	"grunt-contrib-cssmin": "3.0.0",
	"grunt-contrib-uglify": "5.0.0",
	"grunt-contrib-watch": "1.1.0",
	"grunt-phpcs": "0.4.0",
	"grunt-postcss": "0.9.0",
	"grunt-rtlcss": "2.0.2",
	"grunt-sass": "3.1.0",
	"grunt-stylelint": "0.15.0",
	"gruntify-eslint": "5.0.0"
  },
  "browserslist": [
	"> 0.1%",
	"ie 8",
	"ie 9"
  ],
  "publishConfig": {
	"access": "public"
  },
  "engines": {
	"node": "12.18.3",
	"npm": "6.14.4"
  },
  "files": [
	"assets/**/*.{js,scss,php}",
	"includes/**/*.php",
	"i18n/**/*.json",
	"license.txt",
	"wp-ever-accounting.php"
  ]
}
